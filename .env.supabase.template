# Supabase Configuration for Urban-Infra
# Copy this to .env and fill in your Supabase project details

# App Config
APP_NAME=Urban-Infra API
VERSION=0.1.0
DEBUG=false

# Supabase Database
# Get these from your Supabase project settings > Database
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres
SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co
SUPABASE_ANON_KEY=[YOUR-ANON-KEY]
SUPABASE_SERVICE_ROLE_KEY=[YOUR-SERVICE-ROLE-KEY]

# Redis (we can use Upstash Redis for production)
REDIS_URL=redis://localhost:6379

# OpenAI (for agents)
OPENAI_API_KEY=[YOUR-OPENAI-KEY]

# CORS for frontend
ALLOWED_ORIGINS=["http://localhost:3000", "https://[YOUR-VERCEL-DOMAIN].vercel.app"]

# Production deployment
NODE_ENV=production